[% WRAPPER 'layout.html' %]

<div class="DirectoryIndexContainer">
    <p class="about">[% distvname %]の翻訳済ドキュメントの一覧です</p>

    <h1>[% distvname %]</h1>

    <table>
    [% FOR v IN index %]
        <tr>
            <td><a href="./[% v.0 %]">[% v.1 %]</a></td>
            <td>[% v.2 %]</td>
        </tr>
    [% END %]
    </table>
</div>

[% END %]
